      <add name="rule_100" virtualUrl="^~/MobileAdv.html" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Adv/MobileAdv.ashx" ignoreCase="true"/>
      <add name="rule_101" virtualUrl="^~/RegLog.html" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Adv/RegLog.ashx" ignoreCase="true"/>
      <add name="rule_102" virtualUrl="^~/RegVNP.html" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Adv/RegVNP.ashx" ignoreCase="true"/>
      <add name="rule_103" virtualUrl="^~/GetMSISDN.html" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Adv/GetMSISDN.ashx" ignoreCase="true"/>
      <add name="rule_105" virtualUrl="^~/RegPartner.html" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Adv/RegPartner.ashx" ignoreCase="true"/>
    
    </rewrites>
  </urlrewritingnet>
